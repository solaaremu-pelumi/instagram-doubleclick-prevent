{
  "manifest_version": 3,
  "name": "Instagram Double-Tap Preventer",
  "version": "1.0",
  "description": "Prevents accidental double-tap likes on Instagram",
  "host_permissions": [
      "*://*.instagram.com/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
      {
          "matches": ["*://*.instagram.com/*"],
          "js": ["content.js"]
      }
  ]
}